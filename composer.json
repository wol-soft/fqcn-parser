{
    "name": "wol-soft/fqcn-parser",
    "homepage": "https://github.com/wol-soft/fqcn-parser",
    "description": "Parse the FQCN from a PHP file containing a class",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Enno Woortmann",
            "email": "enno.woortmann@web.de"
        }
    ],
    "require": {
        "php": ">=8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5 || ^9.4"
    },
    "autoload": {
        "psr-4": {
            "FQCNParser\\": "src"
        }
    },
    "autoload-dev": {
        "files": [
            "tests/files/GlobalClass.php",
            "tests/files/NamespacedClass.php"
        ],
        "psr-4": {
            "FQCNParser\\Tests\\": "tests"
        }
    }
}
